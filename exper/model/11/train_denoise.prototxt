layer {
  name: "input_data"
  type: "ImageData"
  top: "noisy_data"
  top: "label0"
  image_data_param {
    source: "/home/rgsl888/Finger_Print_dataset/image.txt"
    root_folder: "/home/rgsl888/Finger_Print_dataset/gray_conv_input/"
    batch_size: 8
    #shuffle: true
    shuffle: false
    is_color: false
  }
  transform_param {
    mean_value: 104
    #mirror: true
    mirror: false
  }
}
