layer {
  name: "concat_reduction_fp",
  type: "Deconvolution"
  bottom: "concat_r3"
  top: "concat_reduction_fp"
  convolution_param {
    kernel_size: 5
    stride: 1
    num_output: 1
    pad: 0
    #dilation: 5
    weight_filler: { type: "xavier" }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 1 decay_mult: 0 }
}

layer {
  name: "sum"
  type: "Eltwise"
  bottom: "concat_reduction_fp"
  bottom: "noisy_data"
  top: "prediction"
  eltwise_param {
     operation: 1
  }
}

layer {
       name: "predictionreshp"
       type: "Reshape"
       bottom: "prediction"
       top: "predictionreshp"
       reshape_param {
         shape {
           dim: -1
           dim: 1
         }
       }
}
layer {
       name: "labelreshp"
